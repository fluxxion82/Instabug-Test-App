<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/graph_settings"
            app:startDestination="@id/fragmentSettings">

    <fragment
        android:id="@+id/fragmentSettings"
        android:name="com.example.android.instabugapp.ui.homescreen.settings.SettingsMainFragment"
        tools:layout="@layout/fragment_settings_main">
        <action
            android:id="@+id/action_settings_to_support"
            app:destination="@id/supportFragment"/>
    </fragment>


    <fragment
        android:id="@+id/supportFragment"
        android:name="com.example.android.instabugapp.ui.homescreen.settings.support.SupportFragment"
        android:label="@string/support">
    </fragment>

</navigation>
